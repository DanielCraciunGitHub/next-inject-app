{
  "name": "next-inject",
  "version": "1.2.6",
  "description": "A modular plugin system for Next.js applications, where you can automate mundane configurations with a single click. E.g. next-inject add metadata",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Daniel Craciun",
  "license": "ISC",
  "homepage": "https://www.nextinject.pro",
  "repository": {
    "url": "https://github.com/DanielCraciunGitHub/next-inject-app",
    "directory": "packages/cli"
  },
  "type": "module",
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "typecheck": "tsc --noEmit",
    "dev": "tsup --watch",
    "plugin": "sh -c 'node dist/index.js \"$@\" -c ../../../nextjs-base-template' --",
    "start": "node dist/index.js",
    "pub:release": "pnpm build && pnpm publish --access public --no-git-checks",
    "release": "changeset version",
    "build": "tsup"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.9",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/parser": "^8.2.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-plugin-unused-imports": "^4.1.3",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.12",
    "axios": "^1.7.3",
    "chalk": "^5.3.0",
    "changeset": "^0.2.6",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "execa": "^9.3.0",
    "fast-glob": "^3.3.2",
    "fetch": "^1.1.0",
    "figlet": "^1.7.0",
    "fs-extra": "^11.2.0",
    "latest-version": "^9.0.0",
    "open": "^10.1.0",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "replace-in-file": "^8.1.0",
    "simple-git": "^3.25.0",
    "type-fest": "^4.21.0",
    "ulid": "^2.3.0",
    "zod": "^3.23.8"
  }
}
